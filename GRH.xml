    <!-- Axiomes de CardinalitÃ© -->
    <owl:Class rdf:about="http://GHRJ.com/grh#Employee">
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://GHRJ.com/grh#occupies"/>
